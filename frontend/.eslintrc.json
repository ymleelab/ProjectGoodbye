{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier", "moment"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"prettier/prettier": ["error"],
		"import/no-unresolved": "off",
		"endOfLine": "auto",
		"no-multiple-empty-lines": 2
	}
}
